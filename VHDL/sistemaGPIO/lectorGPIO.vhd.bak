library ieee;
use ieee.std_logic_1164.all;
use ieee.std_logic_unsigned.all; 


entity lectorGPIO is
	port(
		pwm		: in  std_logic; --Hay que coger uno de los relojes con el nombre que aparece en el manual.
		GPIO_0	: in std_logic_vector(1 downto 1)
	);
end lectorGPIO;

architecture a of lectorGPIO is

	signal pwma	: std_logic; --Señal auxiliar. No puedo actuar directamente con las señales del sistema.
	
	begin
	
		pwma<=pwm;
		GPIO_0(0)<=pwma;

end a;